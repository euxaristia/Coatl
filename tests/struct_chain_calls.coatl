struct Point {
  x: i32,
  y: i32
}

fn make_point(a: i32, b: i32) -> Point {
  return Point { x: a, y: b };
}

fn shift_point(p: Point) -> Point {
  return Point { x: p.x + 1, y: p.y + 2 };
}

fn sum_point(p: Point) -> i32 {
  return p.x + p.y;
}

fn main() -> i32 {
  let p: Point = make_point(1, 2);
  let q: Point = shift_point(p);
  return sum_point(q);
}
