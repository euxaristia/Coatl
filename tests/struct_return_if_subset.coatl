struct Point {
  x: i32,
  y: i32
}

fn choose(flag: i32, a: i32, b: i32) -> Point {
  let x: i32 = a + 1;
  let y: i32 = b + 2;
  if (flag == 0) {
    return Point { x: x, y: y };
  }
  return Point { x: x + 10, y: y + 20 };
}

fn main() -> i32 {
  let p: Point = choose(1, 1, 2);
  return p.x + p.y;
}
